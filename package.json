{
  "name": "@apigames/uuid-mongodb",
  "version": "22.1.13",
  "description": "Generates and parses MongoDB BSON UUIDs. Plays nicely with others including the MongoDB native driver and Mongoose.",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "mocha test/**/*.js ",
    "lint": "eslint lib/**/*.js",
    "lint:fix": "eslint --fix lib/**/*.js",
    "win:publish": "mocha test/**/*.js && npm publish",
    "unx:publish": "mocha test/**/*.js && npm publish",
    "win:publish-beta": "mocha test/**/*.js && npm publish --tag beta",
    "unx:publish-beta": "mocha test/**/*.js && npm publish --tag beta",
    "win:publish-dev": "mocha test/**/*.js && npm publish --tag dev",
    "unx:publish-dev": "mocha test/**/*.js && npm publish --tag dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdimascio/uuid-mongodb.git"
  },
  "keywords": [
    "uuid",
    "mongodb"
  ],
  "author": "Carmine DiMascio <cdimascio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdimascio/uuid-mongodb/issues"
  },
  "homepage": "https://github.com/cdimascio/uuid-mongodb#readme",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "mongodb": "3.6.x || 3.7.x || 4.0.x || 4.1.x || 4.2.x || 4.3.x || 4.4.x || 4.5.x || 4.6.x || 4.7.x || 4.8.x || 4.9.x"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^9.2.2",
    "prettier": "^2.7.1"
  }
}
